{
  "name": "notes-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^1.1.6",
    "codemirror": "^5.41.0",
    "node-fetch": "^2.2.0",
    "puppeteer": "^1.18.1",
    "react": "^16.9.0",
    "react-ace": "^7.0.4",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.9.0",
    "react-iframe": "^1.8.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4",
    "rimraf": "^3.0.0",
    "shared": "1.0.0",
    "unzipper": "^0.10.1"
  },
  "workspaces": [
    "extension",
    "shared"
  ],
  "scripts": {
    "build:css": "postcss src/index.css -o src/index.css",
    "watch:css": "postcss src/index.css -o src/index.css -w",
    "start": "npm run watch:css & react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "npm run build",
    "deploy": "aws s3 sync build/ s3://notes-app-client --delete",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E1KTCKT9SOAHBW --paths '/*' && aws cloudfront create-invalidation --distribution-id E3MQXGQ47VCJB0 --paths '/*'",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "postcss-cli": "^6.1.3",
    "autoprefixer": "^9.6.1",
    "tailwindcss": "^1.1.2"
  }
}
